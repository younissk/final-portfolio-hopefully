{% css %}
.experience-section {
  margin-top: 2rem;
}

.experience-item {
  margin-bottom: 3rem;
  padding-left: 2rem;
  position: relative;
}

.experience-item::before {
  content: '';
  position: absolute;
  left: 0;
  top: 0.5rem;
  width: 1.5rem;
  height: 1.5rem;
  background-color: var(--color-primary);
  border-radius: 4px;
}

.experience-title {
  font-size: 1.75rem;
  font-weight: bold;
  color: var(--heading-color);
  margin-bottom: 0.5rem;
}

.experience-company {
  font-size: 1.25rem;
  color: var(--color-secondary);
  margin-bottom: 0.5rem;
}

.experience-date {
  font-size: 1rem;
  color: var(--color-secondary);
  margin-bottom: 1rem;
}

.experience-description {
  color: var(--text-color);
  line-height: 1.6;
  margin-top: 0.5rem;
}

@media (prefers-color-scheme: dark) {
  .experience-company {
    color: #9ca3af;
  }

  .experience-date {
    color: #9ca3af;
  }

  .experience-description {
    color: #e5e7eb;
  }
}

@media (max-width: 768px) {
  .experience-item {
    padding-left: 1.5rem;
  }

  .experience-title {
    font-size: 1.5rem;
  }

  .experience-company {
    font-size: 1.1rem;
  }
}
{% endcss %}